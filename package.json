{
  "name": "retrites",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/",
    "start": "react-scripts start npm run build-css",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom --testResultsProcessor='jest-junit' --verbose",
    "test:watch": "react-scripts test --env=jsdom --verbose"
  },
  "jest-junit": {
    "output": "./test-results.xml",
    "suiteName": "jest tests",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º "
  },
  "dependencies": {
    "antd": "^3.25.3",
    "axios": "^0.19.0",
    "classnames": "2.2.6",
    "date-fns": "1.29.0",
    "debounce-promise": "3.1.0",
    "downshift": "2.0.19",
    "faker": "4.1.0",
    "history": "4.7.2",
    "moment": "^2.24.0",
    "normalize.css": "8.0.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-google-recaptcha": "^2.0.1",
    "react-loadable": "^5.4.0",
    "react-localize-redux": "3.5.2",
    "react-recaptcha-google": "^1.1.1",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "reactstrap": "7.1.0",
    "recharts": "^1.7.0",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest-enzyme": "6.0.2",
    "jest-junit": "4.0.0",
    "node-sass": "^4.12.0",
    "redux-mock-store": "1.5.3",
    "sass-loader": "^7.1.0",
    "shortid": "2.2.12"
  }
